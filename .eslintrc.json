{
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "extends": ["eslint:recommended"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["import"],
    "settings": {},
    "globals": {
        "process": "readonly"
    },
    "rules": {
        "quotes": ["warn", "double"],
        "no-unused-vars": "warn",
        "require-await": "warn",
        "no-alert": "error",
        "no-undef": "error",
        "object-shorthand": ["warn", "never"],
        "no-underscore-dangle": "error",
        "no-eval": "error",
        "import/no-useless-path-segments": ["error", { "noUselessIndex": true }],
        "import/extensions": ["error", { "js": "always" }],
        "no-case-declarations": "off",
        "no-prototype-builtins": "off",
        "no-inner-declarations": "off",
        "no-extra-semi": "off",
        "no-empty": "off"
    }
}
